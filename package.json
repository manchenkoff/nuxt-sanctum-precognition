{
  "name": "nuxt-sanctum-precognition",
  "version": "0.0.10",
  "description": "Nuxt module for Laravel Precognition integration",
  "homepage": "https://manchenkoff.gitbook.io/nuxt-sanctum-precognition/",
  "repository": "manchenkoff/nuxt-sanctum-precognition",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "rc": "pnpm pack --pack-destination dist",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "validate": "npm run lint && npm run test:types && npm run test"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "lodash-es": "^4.17.21",
    "nuxt-auth-sanctum": "^1.2.0",
    "object-form-encoder": "^0.0.6"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.3",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/kit": "^4.0.3",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.0.3",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui": "^3.3.2",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.34.0",
    "nuxt": "^4.0.3",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vue": "^3.5.20",
    "vue-tsc": "^2.2.12"
  },
  "packageManager": "pnpm@10.15.0+sha512.486ebc259d3e999a4e8691ce03b5cac4a71cbeca39372a9b762cb500cfdf0873e2cb16abe3d951b1ee2cf012503f027b98b6584e4df22524e0c7450d9ec7aa7b",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}
