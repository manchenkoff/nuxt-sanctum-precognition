{
  "name": "nuxt-sanctum-precognition",
  "version": "0.0.9",
  "description": "Nuxt module for Laravel Precognition integration",
  "homepage": "https://manchenkoff.gitbook.io/nuxt-sanctum-precognition/",
  "repository": "manchenkoff/nuxt-sanctum-precognition",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "rc": "pnpm pack --pack-destination dist",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "validate": "npm run lint && npm run test:types && npm run test"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "lodash-es": "^4.17.21",
    "nuxt-auth-sanctum": "^1.1.1",
    "object-form-encoder": "^0.0.6"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.2",
    "@nuxt/eslint-config": "^1.7.0",
    "@nuxt/kit": "^4.0.1",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^4.0.1",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui": "^3.2.0",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.1.0",
    "changelogen": "^0.6.2",
    "eslint": "^9.31.0",
    "nuxt": "^4.0.1",
    "typescript": "^5.8.3",
    "vitest": "^3.2.4",
    "vue-tsc": "^2.2.12"
  },
  "packageManager": "pnpm@10.13.1+sha512.37ebf1a5c7a30d5fabe0c5df44ee8da4c965ca0c5af3dbab28c3a1681b70a256218d05c81c9c0dcf767ef6b8551eb5b960042b9ed4300c59242336377e01cfad",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ],
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    }
  }
}
