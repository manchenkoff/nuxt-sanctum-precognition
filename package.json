{
  "name": "nuxt-sanctum-precognition",
  "version": "0.0.11",
  "description": "Nuxt module for Laravel Precognition integration",
  "homepage": "https://manchenkoff.gitbook.io/nuxt-sanctum-precognition/",
  "repository": "manchenkoff/nuxt-sanctum-precognition",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "typesVersions": {
    "*": {
      ".": [
        "./dist/types.d.mts"
      ]
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "rc": "pnpm pack --pack-destination dist",
    "dev": "npm run dev:prepare && nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "validate": "npm run lint && npm run test:types && npm run test"
  },
  "dependencies": {
    "@nuxt/kit": "^4.1.2",
    "defu": "^6.1.4",
    "lodash-es": "^4.17.21",
    "nuxt-auth-sanctum": "^1.3.0",
    "object-form-encoder": "^0.0.6"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.6.5",
    "@nuxt/eslint-config": "^1.9.0",
    "@nuxt/module-builder": "^1.0.2",
    "@nuxt/schema": "^4.1.2",
    "@nuxt/test-utils": "^3.19.2",
    "@nuxt/ui": "^3.3.4",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.5.2",
    "changelogen": "^0.6.2",
    "eslint": "^9.36.0",
    "nuxt": "^4.1.2",
    "typescript": "^5.9.2",
    "vitest": "^3.2.4",
    "vue": "^3.5.21",
    "vue-tsc": "^3.0.7"
  },
  "packageManager": "pnpm@10.17.0+sha512.fce8a3dd29a4ed2ec566fb53efbb04d8c44a0f05bc6f24a73046910fb9c3ce7afa35a0980500668fa3573345bd644644fa98338fa168235c80f4aa17aa17fbef",
  "pnpm": {
    "onlyBuiltDependencies": [
      "@parcel/watcher",
      "@tailwindcss/oxide",
      "esbuild",
      "unrs-resolver",
      "vue-demi"
    ]
  }
}
