{
  "name": "nuxt-sanctum-precognition",
  "version": "0.0.1",
  "description": "Nuxt module for Laravel Precognition integration",
  "homepage": "https://manchenkoff.gitbook.io/nuxt-sanctum-precognition/",
  "repository": "manchenkoff/nuxt-sanctum-precognition",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.ts",
      "import": "./dist/module.mjs",
      "require": "./dist/module.cjs"
    }
  },
  "main": "./dist/module.cjs",
  "types": "./dist/types.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "validate": "npm run lint && npm run test:types && npm run test"
  },
  "devDependencies": {
    "@nuxt/devtools": "latest",
    "@nuxt/eslint-config": "^1.0.1",
    "@nuxt/kit": "^3.15.4",
    "@nuxt/module-builder": "^0.8.4",
    "@nuxt/schema": "^3.15.4",
    "@nuxt/test-utils": "^3.15.4",
    "@types/node": "latest",
    "changelogen": "^0.5.7",
    "eslint": "^9.20.0",
    "nuxt": "^3.15.4",
    "nuxt-auth-sanctum": "^0.5.4",
    "typescript": "^5.7.3",
    "vitest": "^3.0.5",
    "vue-tsc": "^2.1.10"
  },
  "packageManager": "pnpm@10.2.1+sha512.398035c7bd696d0ba0b10a688ed558285329d27ea994804a52bad9167d8e3a72bcb993f9699585d3ca25779ac64949ef422757a6c31102c12ab932e5cbe5cc92"
}
