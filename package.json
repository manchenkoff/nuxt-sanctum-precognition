{
  "name": "nuxt-sanctum-precognition",
  "version": "0.0.4",
  "description": "Nuxt module for Laravel Precognition integration",
  "homepage": "https://manchenkoff.gitbook.io/nuxt-sanctum-precognition/",
  "repository": "manchenkoff/nuxt-sanctum-precognition",
  "license": "MIT",
  "type": "module",
  "exports": {
    ".": {
      "types": "./dist/types.d.mts",
      "import": "./dist/module.mjs"
    }
  },
  "main": "./dist/module.mjs",
  "files": [
    "dist"
  ],
  "scripts": {
    "prepack": "nuxt-module-build build",
    "rc": "pnpm pack --pack-destination dist",
    "dev": "nuxi dev playground",
    "dev:build": "nuxi build playground",
    "dev:prepare": "nuxt-module-build build --stub && nuxt-module-build prepare && nuxi prepare playground",
    "release": "npm run lint && npm run test && npm run prepack && changelogen --release && npm publish && git push --follow-tags",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "vitest run",
    "test:watch": "vitest watch",
    "test:types": "vue-tsc --noEmit && cd playground && vue-tsc --noEmit",
    "validate": "npm run lint && npm run test:types && npm run test"
  },
  "dependencies": {
    "defu": "^6.1.4",
    "nuxt-auth-sanctum": "^0.6.6",
    "object-form-encoder": "^0.0.6",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@nuxt/devtools": "^2.4.0",
    "@nuxt/eslint-config": "^1.3.1",
    "@nuxt/kit": "^3.17.3",
    "@nuxt/module-builder": "^1.0.1",
    "@nuxt/schema": "^3.17.3",
    "@nuxt/test-utils": "^3.18.0",
    "@nuxt/ui": "^3.1.1",
    "@types/lodash": "^4.17.16",
    "@types/node": "^22.15.17",
    "changelogen": "^0.6.1",
    "eslint": "^9.26.0",
    "nuxt": "^3.17.3",
    "typescript": "^5.8.3",
    "vitest": "^3.1.3",
    "vue-tsc": "^2.2.10"
  },
  "packageManager": "pnpm@10.10.0+sha512.d615db246fe70f25dcfea6d8d73dee782ce23e2245e3c4f6f888249fb568149318637dca73c2c5c8ef2a4ca0d5657fb9567188bfab47f566d1ee6ce987815c39",
  "pnpm": {
    "onlyBuiltDependencies": [
      "esbuild"
    ],
    "overrides": {
      "esbuild@<=0.24.2": ">=0.25.0"
    }
  }
}
